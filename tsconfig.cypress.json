{
    "extends": "tsconfig.json",
    "compilerOptions": {
      "isolatedModules": false,
      "strict": true,
      "target": "es5",
      "lib": ["es5", "dom"],
      "types": ["cypress"],
      "noEmit": false,
      "baseUrl": "./node_modules"
    },
    "include": [
      "./node_modules/cypress",
      "src/**/*.spec.ts",
      "src/**/*.spec.tsx"
    ],
  }
